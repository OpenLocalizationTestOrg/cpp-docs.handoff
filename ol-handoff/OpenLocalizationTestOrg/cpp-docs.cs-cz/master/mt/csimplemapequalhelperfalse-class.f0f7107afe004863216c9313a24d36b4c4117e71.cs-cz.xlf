        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source><bpt id="p1">[</bpt>CSimpleMapEqualHelperFalse::IsEqualValue<ept id="p1">](#csimplemapequalhelperfalse__isequalvalue)</ept></source>
