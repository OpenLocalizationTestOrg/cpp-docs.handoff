          <source>Call <bpt id="p1">[</bpt>CInternetFile::Read<ept id="p1">](../mfc/reference/cinternetfile-class.md#cinternetfile__read)</ept>.</source>
        </trans-unit>
        <trans-unit id="166" translate="yes" xml:space="preserve">
          <source>Get information about an HTTP request.</source>
        </trans-unit>
        <trans-unit id="167" translate="yes" xml:space="preserve">
          <source>Establish an HTTP connection.</source>
        </trans-unit>
        <trans-unit id="168" translate="yes" xml:space="preserve">
          <source>Call <bpt id="p1">[</bpt>CHttpConnection::OpenRequest<ept id="p1">](../mfc/reference/chttpconnection-class.md#chttpconnection__openrequest)</ept> to create a <bpt id="p2">[</bpt>CHttpFile<ept id="p2">](../mfc/reference/chttpfile-class.md)</ept> object.</source>
        </trans-unit>
        <trans-unit id="169" translate="yes" xml:space="preserve">
          <source>Call <bpt id="p1">[</bpt>CHttpFile::QueryInfo<ept id="p1">](../mfc/reference/chttpfile-class.md#chttpfile__queryinfo)</ept>.</source>
        </trans-unit>
        <trans-unit id="170" translate="yes" xml:space="preserve">
