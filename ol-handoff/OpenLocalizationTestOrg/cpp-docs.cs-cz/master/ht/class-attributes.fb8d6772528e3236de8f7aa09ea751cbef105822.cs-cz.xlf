        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The following attributes apply to the <bpt id="p1">[</bpt>class<ept id="p1">](../cpp/class-cpp.md)</ept> C++ keyword.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
