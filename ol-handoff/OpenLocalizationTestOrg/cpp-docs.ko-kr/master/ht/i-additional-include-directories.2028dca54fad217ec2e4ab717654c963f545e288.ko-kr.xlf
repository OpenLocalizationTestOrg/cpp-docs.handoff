        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>Directories specified with the <bpt id="p1">**</bpt>/I<ept id="p1">**</ept> option, in the order that CL encounters them.</source>
        </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>Directories specified in the <bpt id="p1">**</bpt>INCLUDE<ept id="p1">**</ept> environment variable.</source>
        </trans-unit>
