        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Callback Function for CDC::GrayString</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source><bpt id="p1">*</bpt>OutputFunc<ept id="p1">*</ept> is a placeholder for the application-supplied callback function name.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Identifies a memory device context with a bitmap of at least the width and height specified by <ph id="ph1">`nWidth`</ph> and <ph id="ph2">`nHeight`</ph> to <ph id="ph3">`GrayString`</ph>.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Points to the character string to be drawn.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Specifies the number of characters to output.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Return Value</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>The callback function's return value must be <bpt id="p1">**</bpt>TRUE<ept id="p1">**</ept> to indicate success; otherwise it is <bpt id="p2">**</bpt>FALSE<ept id="p2">**</ept>.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Remarks</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>The callback function (<bpt id="p1">*</bpt>OutputFunc<ept id="p1">*</ept>) must draw an image relative to the coordinates (0,0) rather than (<bpt id="p2">*</bpt>x<ept id="p2">*</ept>, <bpt id="p3">*</bpt>y<ept id="p3">*</ept>).</source>
        </trans-unit>
