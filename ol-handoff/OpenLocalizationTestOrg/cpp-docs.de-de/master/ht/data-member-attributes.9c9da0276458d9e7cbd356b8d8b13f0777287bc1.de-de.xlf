          <source>db_accessor</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Groups <bpt id="p1">**</bpt>db_column<ept id="p1">**</ept> attributes that participate in <ph id="ph1">`IAccessor`</ph>-based binding.</source>
        </trans-unit>
