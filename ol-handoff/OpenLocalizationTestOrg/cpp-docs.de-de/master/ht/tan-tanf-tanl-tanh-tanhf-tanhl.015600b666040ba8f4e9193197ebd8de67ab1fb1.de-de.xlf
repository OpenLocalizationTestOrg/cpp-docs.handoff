        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Calculates the tangent (<ph id="ph1">`tan`</ph>, <ph id="ph2">`tanf`</ph>, or <ph id="ph3">`tanl`</ph>), or hyperbolic tangent (<ph id="ph4">`tanh`</ph>, <ph id="ph5">`tanhf`</ph>, or <ph id="ph6">`tanhl`</ph>).</source>
        </trans-unit>
