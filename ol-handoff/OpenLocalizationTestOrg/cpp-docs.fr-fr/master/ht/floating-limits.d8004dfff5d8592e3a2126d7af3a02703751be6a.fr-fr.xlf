          <source>FLT_MAX DBL_MAX LDBL_MAX</source>
        </trans-unit>
        <trans-unit id="123" translate="yes" xml:space="preserve">
          <source>Maximum representable  floating-point number.</source>
        </trans-unit>
        <trans-unit id="124" translate="yes" xml:space="preserve">
          <source>3.402823466e+38F 1.7976931348623158e+308 1.7976931348623158e+308</source>
