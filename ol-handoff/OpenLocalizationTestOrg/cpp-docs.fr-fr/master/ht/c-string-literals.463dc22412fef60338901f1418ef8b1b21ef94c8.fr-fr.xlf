          <source><bpt id="p1"> **</bpt>"<ept id="p1">**</ept> <bpt id="p2">*</bpt>s-char-sequence<ept id="p2">*</ept> opt<bpt id="p3">**</bpt>"<ept id="p3">**</ept></source>
