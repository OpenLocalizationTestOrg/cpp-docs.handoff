        <trans-unit id="150" translate="yes" xml:space="preserve">
          <source>Adds a band to a rebar.</source>
        </trans-unit>
        <trans-unit id="151" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="152" translate="yes" xml:space="preserve">
          <source>[in] [out] <ph id="ph1">`pBar`</ph></source>
        </trans-unit>
        <trans-unit id="153" translate="yes" xml:space="preserve">
          <source>A pointer to the child window that is to be inserted into the rebar.</source>
        </trans-unit>
        <trans-unit id="154" translate="yes" xml:space="preserve">
          <source>The referenced object must have the <bpt id="p1">**</bpt>WS_CHILD<ept id="p1">**</ept> window style.</source>
        </trans-unit>
        <trans-unit id="155" translate="yes" xml:space="preserve">
          <source>[in] <ph id="ph1">`pszText`</ph></source>
        </trans-unit>
        <trans-unit id="156" translate="yes" xml:space="preserve">
          <source>Specifies the text to appear on the rebar.</source>
        </trans-unit>
        <trans-unit id="157" translate="yes" xml:space="preserve">
          <source>The text is not part of the child window.</source>
        </trans-unit>
        <trans-unit id="158" translate="yes" xml:space="preserve">
          <source>Rather, it is displayed on the rebar itself.</source>
        </trans-unit>
        <trans-unit id="159" translate="yes" xml:space="preserve">
          <source>[in] [out] <ph id="ph1">`pbmp`</ph></source>
        </trans-unit>
        <trans-unit id="160" translate="yes" xml:space="preserve">
          <source>Specifies the bitmap to be displayed on the rebar background.</source>
        </trans-unit>
        <trans-unit id="161" translate="yes" xml:space="preserve">
          <source>[in] <ph id="ph1">`dwStyle`</ph></source>
        </trans-unit>
        <trans-unit id="162" translate="yes" xml:space="preserve">
          <source>Contains the style to apply to the band.</source>
        </trans-unit>
        <trans-unit id="163" translate="yes" xml:space="preserve">
          <source>For a complete list of band styles, see the description for <ph id="ph1">`fStyle`</ph> in the <bpt id="p1">[</bpt>REBARBANDINFO<ept id="p1">](http://msdn.microsoft.com/library/windows/desktop/bb774393)</ept> structure in the Windows SDK documentation.</source>
        </trans-unit>
        <trans-unit id="164" translate="yes" xml:space="preserve">
          <source>[in] <ph id="ph1">`clrFore`</ph></source>
        </trans-unit>
        <trans-unit id="165" translate="yes" xml:space="preserve">
          <source>Represents the foreground color of the rebar.</source>
        </trans-unit>
        <trans-unit id="166" translate="yes" xml:space="preserve">
          <source>[in] <ph id="ph1">`clrBack`</ph></source>
        </trans-unit>
        <trans-unit id="167" translate="yes" xml:space="preserve">
          <source>Represents the background color of the rebar.</source>
        </trans-unit>
