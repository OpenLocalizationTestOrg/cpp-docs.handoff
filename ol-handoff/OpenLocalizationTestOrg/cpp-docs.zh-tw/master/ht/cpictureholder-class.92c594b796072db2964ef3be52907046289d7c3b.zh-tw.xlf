        <trans-unit id="198" translate="yes" xml:space="preserve">
          <source>Return Value</source>
        </trans-unit>
        <trans-unit id="199" translate="yes" xml:space="preserve">
          <source>A pointer to the <ph id="ph1">`CPictureHolder`</ph> object's <ph id="ph2">`IPictureDisp`</ph> interface.</source>
        </trans-unit>
        <trans-unit id="200" translate="yes" xml:space="preserve">
          <source>Remarks</source>
        </trans-unit>
        <trans-unit id="201" translate="yes" xml:space="preserve">
          <source>The caller must call <bpt id="p1">**</bpt>Release<ept id="p1">**</ept> on this pointer when finished with it.</source>
        </trans-unit>
