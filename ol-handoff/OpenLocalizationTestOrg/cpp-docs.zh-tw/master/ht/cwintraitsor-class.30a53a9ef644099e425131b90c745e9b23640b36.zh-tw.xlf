        <trans-unit id="128" translate="yes" xml:space="preserve">
          <source>Return Value</source>
        </trans-unit>
        <trans-unit id="129" translate="yes" xml:space="preserve">
          <source>A combination of styles that are passed in <ph id="ph1">`dwStyle`</ph> and the default ones specified by <ph id="ph2">`t_dwStyle`</ph>, using the logical OR operator.</source>
        </trans-unit>
