        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Stores the pointer to the current virtual-machine control structure (VMCS) at the specified address.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
