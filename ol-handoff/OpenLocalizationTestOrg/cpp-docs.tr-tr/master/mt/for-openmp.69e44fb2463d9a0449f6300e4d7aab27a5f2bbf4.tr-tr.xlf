          <source>where,</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`clause`</ph> (optional)</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Zero or more clauses.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>See the Remarks section for a list of the clauses supported by <bpt id="p1">**</bpt>for<ept id="p1">**</ept>.</source>
        </trans-unit>
