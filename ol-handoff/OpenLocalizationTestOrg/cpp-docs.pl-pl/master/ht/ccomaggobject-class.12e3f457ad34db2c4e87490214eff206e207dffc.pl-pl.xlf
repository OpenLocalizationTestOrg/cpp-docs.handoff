        <trans-unit id="163" translate="yes" xml:space="preserve">
          <source>[out] A pointer to a <bpt id="p1">**</bpt>CComAggObject<ph id="ph1">\&lt;</ph><ept id="p1">**</ept><bpt id="p2">*</bpt>contained<ept id="p2">*</ept> <bpt id="p3">**</bpt><ph id="ph2">&gt;</ph><ept id="p3">**</ept> pointer.</source>
        </trans-unit>
        <trans-unit id="164" translate="yes" xml:space="preserve">
          <source>If <ph id="ph1">`CreateInstance`</ph> is unsuccessful, <ph id="ph2">`pp`</ph> is set to <bpt id="p1">**</bpt>NULL<ept id="p1">**</ept>.</source>
