        <trans-unit id="174" translate="yes" xml:space="preserve">
          <source>CCmdUI::ContinueRouting</source>
        </trans-unit>
        <trans-unit id="175" translate="yes" xml:space="preserve">
          <source>Call this member function to tell the command-routing mechanism to continue routing the current message down the chain of handlers.</source>
        </trans-unit>
