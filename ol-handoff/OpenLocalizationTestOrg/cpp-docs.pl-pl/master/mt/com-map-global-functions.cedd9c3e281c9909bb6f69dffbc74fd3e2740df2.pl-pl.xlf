          <source><bpt id="p1">&lt;a name="inlineisequaliunknown"&gt;</bpt><ept id="p1">&lt;/a&gt;</ept>  InlineIsEqualIUnknown</source>
        </trans-unit>
        <trans-unit id="124" translate="yes" xml:space="preserve">
          <source>Call this function, for the special case of testing for <bpt id="p1">**</bpt>IUnknown<ept id="p1">**</ept>.</source>
        </trans-unit>
        <trans-unit id="125" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="126" translate="yes" xml:space="preserve">
          <source><bpt id="p1">*</bpt>rguid1<ept id="p1">*</ept></source>
        </trans-unit>
        <trans-unit id="127" translate="yes" xml:space="preserve">
          <source>[in] The GUID to compare to <bpt id="p1">**</bpt>IID_IUnknown<ept id="p1">**</ept>.</source>
        </trans-unit>
