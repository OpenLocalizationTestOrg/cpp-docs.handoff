        <trans-unit id="123" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="124" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="125" translate="yes" xml:space="preserve">
          <source>[out] <ph id="ph1">`pEnabled`</ph></source>
        </trans-unit>
        <trans-unit id="126" translate="yes" xml:space="preserve">
          <source><ph id="ph1"> `TRUE`</ph> indicates that the registry information is directed to the <bpt id="p1">**</bpt>HKCU<ept id="p1">**</ept> node; <ph id="ph2">`FALSE`</ph> indicates that the application writes registry information to the default node.</source>
        </trans-unit>
        <trans-unit id="127" translate="yes" xml:space="preserve">
          <source>The default node is <bpt id="p1">**</bpt>HKEY_CLASSES_ROOT<ept id="p1">**</ept> (<bpt id="p2">**</bpt>HKCR<ept id="p2">**</ept>).</source>
        </trans-unit>
        <trans-unit id="128" translate="yes" xml:space="preserve">
          <source>Return Value</source>
        </trans-unit>
        <trans-unit id="129" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`S_OK`</ph> if the method is successful, otherwise the <ph id="ph2">`HRESULT`</ph> error code if an error occurs.</source>
        </trans-unit>
        <trans-unit id="130" translate="yes" xml:space="preserve">
          <source>Remarks</source>
        </trans-unit>
        <trans-unit id="131" translate="yes" xml:space="preserve">
          <source>Registry redirection is not enabled by default.</source>
        </trans-unit>
        <trans-unit id="132" translate="yes" xml:space="preserve">
          <source>If you enable this option, registry access is redirected to <bpt id="p1">**</bpt>HKEY_CURRENT_USER\Software\Classes<ept id="p1">**</ept>.</source>
        </trans-unit>
        <trans-unit id="133" translate="yes" xml:space="preserve">
