        </trans-unit>
        <trans-unit id="136" translate="yes" xml:space="preserve">
          <source>This property sets the user interface item for this command to the appropriate check state.</source>
        </trans-unit>
        <trans-unit id="137" translate="yes" xml:space="preserve">
          <source>Set Check to the following values:</source>
        </trans-unit>
        <trans-unit id="138" translate="yes" xml:space="preserve">
          <source>0 Uncheck</source>
        </trans-unit>
        <trans-unit id="139" translate="yes" xml:space="preserve">
          <source>1 Check</source>
        </trans-unit>
        <trans-unit id="140" translate="yes" xml:space="preserve">
          <source>2 Set indeterminate</source>
