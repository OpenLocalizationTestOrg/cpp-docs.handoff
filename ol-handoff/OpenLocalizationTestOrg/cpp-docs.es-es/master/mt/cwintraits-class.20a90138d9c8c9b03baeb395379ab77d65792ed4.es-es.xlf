        <trans-unit id="138" translate="yes" xml:space="preserve">
          <source>If <ph id="ph1">`dwStyle`</ph> is 0, the template style values ( <ph id="ph2">`t_dwStyle`</ph>) are returned.</source>
        </trans-unit>
        <trans-unit id="139" translate="yes" xml:space="preserve">
          <source>If <ph id="ph1">`dwStyle`</ph> is nonzero, <ph id="ph2">`dwStyle`</ph> is returned.</source>
        </trans-unit>
