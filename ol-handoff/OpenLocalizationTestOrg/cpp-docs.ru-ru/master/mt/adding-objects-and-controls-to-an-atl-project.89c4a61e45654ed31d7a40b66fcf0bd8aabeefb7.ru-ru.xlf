          <source><bpt id="p1">[</bpt>ATL Property Page<ept id="p1">](../../atl/reference/atl-property-page-wizard.md)</ept></source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source><bpt id="p1">[</bpt>ATL Active Server Page Component<ept id="p1">](../../atl/reference/atl-active-server-page-component-wizard.md)</ept></source>
