        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>atan, atanf, atanl, atan2, atan2f, atan2l | Microsoft Docs</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>atan, atanf, atanl, atan2, atan2f, atan2l</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Calculates the arctangent of <ph id="ph1">`x`</ph> (<ph id="ph2">`atan`</ph>, <ph id="ph3">`atanf`</ph>, and <ph id="ph4">`atanl`</ph>) or the arctangent of <ph id="ph5">`y`/`x`</ph> (<ph id="ph6">`atan2`</ph>, <ph id="ph7">`atan2f`</ph>, and <ph id="ph8">`atan2l`</ph>).</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>,</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Any numbers.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Return Value</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>returns the arctangent of <ph id="ph1">`x`</ph> in the range –π/2 to π/2 radians.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>returns the arctangent of <ph id="ph1">`y/x`</ph> in the range –π to π radians.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>If <ph id="ph1">`x`</ph> is 0, <ph id="ph2">`atan`</ph> returns 0.</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>If both parameters of <ph id="ph1">`atan2`</ph> are 0, the function returns 0.</source>
        </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>All results are in radians.</source>
        </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>uses the signs of both parameters to determine the quadrant of the return value.</source>
        </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>Input</source>
        </trans-unit>
