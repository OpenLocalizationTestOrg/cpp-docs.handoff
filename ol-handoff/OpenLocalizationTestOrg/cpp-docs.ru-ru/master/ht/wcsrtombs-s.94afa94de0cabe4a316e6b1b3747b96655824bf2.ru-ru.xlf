          <source>[in]</source>
        </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>Points to the wide character string to be converted.</source>
        </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
