          <source>Identities defining the complex cosines:</source>
        </trans-unit>
        <trans-unit id="162" translate="yes" xml:space="preserve">
          <source>cos ( <bpt id="p1">*</bpt>z<ept id="p1">*</ept>) = (1/2)<ph id="ph1">\*</ph>( exp ( <bpt id="p2">*</bpt>iz<ept id="p2">*</ept>) + exp (- <bpt id="p3">*</bpt>iz<ept id="p3">*</ept>) )</source>
        </trans-unit>
        <trans-unit id="163" translate="yes" xml:space="preserve">
          <source>cos ( <bpt id="p1">*</bpt>z<ept id="p1">*</ept>) = cos ( <bpt id="p2">*</bpt>a<ept id="p2">*</ept><ph id="ph1"> + </ph><bpt id="p3">*</bpt>bi<ept id="p3">*</ept>) = cos ( <bpt id="p4">*</bpt>a<ept id="p4">*</ept>) cosh ( <bpt id="p5">*</bpt>b<ept id="p5">*</ept>) - isin ( <bpt id="p6">*</bpt>a<ept id="p6">*</ept>) sinh ( <bpt id="p7">*</bpt>b<ept id="p7">*</ept>)</source>
        </trans-unit>
