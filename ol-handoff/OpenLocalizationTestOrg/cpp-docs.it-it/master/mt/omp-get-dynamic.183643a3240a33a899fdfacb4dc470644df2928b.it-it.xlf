        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>omp_get_dynamic</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Returns a value that indicates if the number of threads available in subsequent parallel region can be adjusted by the run time.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Return Value</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>If nonzero, dynamic adjustment of threads is enabled.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Remarks</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Dynamic adjustment of threads is specified with <bpt id="p1">[</bpt>omp_set_dynamic<ept id="p1">](../../../parallel/openmp/reference/omp-set-dynamic.md)</ept> and <bpt id="p2">[</bpt>OMP_DYNAMIC<ept id="p2">](../../../parallel/openmp/reference/omp-dynamic.md)</ept>.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>For more information, see <bpt id="p1">[</bpt>3.1.7 omp_set_dynamic Function<ept id="p1">](../../../parallel/openmp/3-1-7-omp-set-dynamic-function.md)</ept>.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Example</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
