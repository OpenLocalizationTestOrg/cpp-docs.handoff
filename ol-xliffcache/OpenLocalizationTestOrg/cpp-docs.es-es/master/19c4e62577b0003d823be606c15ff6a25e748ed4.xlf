          <source>CMFCTasksPaneTaskGroup::m_strName</source>
        </trans-unit>
        <trans-unit id="193" translate="yes" xml:space="preserve">
          <source>Specifies the name of the group.</source>
        </trans-unit>
        <trans-unit id="194" translate="yes" xml:space="preserve">
          <source>Remarks</source>
        </trans-unit>
        <trans-unit id="195" translate="yes" xml:space="preserve">
          <source>If this value is empty, the group caption is not displayed and the group cannot be collapsed.</source>
        </trans-unit>
        <trans-unit id="196" translate="yes" xml:space="preserve">
          <source>CMFCTasksPaneTaskGroup::SetACCData</source>
        </trans-unit>
        <trans-unit id="197" translate="yes" xml:space="preserve">
