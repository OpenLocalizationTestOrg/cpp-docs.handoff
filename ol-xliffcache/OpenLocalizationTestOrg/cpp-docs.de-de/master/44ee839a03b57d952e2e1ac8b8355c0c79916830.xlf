        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>These files are used to build a precompiled header file <bpt id="p1">*</bpt>Projname<ept id="p1">*</ept>.pch and a precompiled types file Stdafx.obj.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>These files are located in the <bpt id="p1">*</bpt>Projname<ept id="p1">*</ept> directory.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
