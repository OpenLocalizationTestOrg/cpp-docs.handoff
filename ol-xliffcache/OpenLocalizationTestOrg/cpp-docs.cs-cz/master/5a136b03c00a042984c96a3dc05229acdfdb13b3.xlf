        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Releases the interface associated with this <ph id="ph1">`ComPtr`</ph> object and then retrieves the address of the <ph id="ph2">`ComPtr`</ph> object.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
