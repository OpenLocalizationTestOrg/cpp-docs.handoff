        <trans-unit id="155" translate="yes" xml:space="preserve">
          <source>Places an interface in the .idl file as a dispatch interface.</source>
        </trans-unit>
        <trans-unit id="156" translate="yes" xml:space="preserve">
          <source>displaybind</source>
        </trans-unit>
        <trans-unit id="157" translate="yes" xml:space="preserve">
          <source>Indicates a property that should be displayed to the user as bindable.</source>
        </trans-unit>
        <trans-unit id="158" translate="yes" xml:space="preserve">
          <source>dual</source>
        </trans-unit>
        <trans-unit id="159" translate="yes" xml:space="preserve">
          <source>Places an interface in the .idl file as a dual interface.</source>
        </trans-unit>
