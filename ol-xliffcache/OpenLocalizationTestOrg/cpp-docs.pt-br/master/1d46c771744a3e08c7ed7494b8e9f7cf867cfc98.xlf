        <trans-unit id="230" translate="yes" xml:space="preserve">
          <source>Indicates that an interface member should not be displayed in a property browser.</source>
        </trans-unit>
        <trans-unit id="231" translate="yes" xml:space="preserve">
          <source>noncreatable</source>
