          <source>do_get</source>
        </trans-unit>
        <trans-unit id="126" translate="yes" xml:space="preserve">
          <source>A virtual function called to retrieve the message catalog.</source>
        </trans-unit>
        <trans-unit id="127" translate="yes" xml:space="preserve">
