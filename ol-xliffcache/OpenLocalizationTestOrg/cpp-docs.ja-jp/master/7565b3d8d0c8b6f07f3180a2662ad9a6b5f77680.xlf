        </trans-unit>
        <trans-unit id="122" translate="yes" xml:space="preserve">
          <source>Closes the message catalog.</source>
        </trans-unit>
        <trans-unit id="123" translate="yes" xml:space="preserve">
          <source>do_close</source>
        </trans-unit>
        <trans-unit id="124" translate="yes" xml:space="preserve">
          <source>A virtual function called to lose the message catalog.</source>
        </trans-unit>
