        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>memory_order Enum</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Supplies symbolic names for synchronization operations on memory locations.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>These operations affect how assignments in one thread become visible in another.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Remarks</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>No ordering required.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>A load operation acts as a consume operation on the memory location.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>A load operation acts as an acquire operation on the memory location.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>A store operation acts as a release operation on the memory location.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Combines <ph id="ph1">`memory_order_acquire`</ph> and <ph id="ph2">`memory_order_release`</ph>.</source>
        </trans-unit>
