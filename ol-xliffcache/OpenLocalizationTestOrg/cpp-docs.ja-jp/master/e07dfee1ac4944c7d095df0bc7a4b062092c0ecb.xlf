        <trans-unit id="206" translate="yes" xml:space="preserve">
          <source>Index to the ACE entry to remove.</source>
        </trans-unit>
        <trans-unit id="207" translate="yes" xml:space="preserve">
          <source>Remarks</source>
        </trans-unit>
        <trans-unit id="208" translate="yes" xml:space="preserve">
          <source>This method is derived from <bpt id="p1">[</bpt>CAtlArray::RemoveAt<ept id="p1">](../../atl/reference/catlarray-class.md#catlarray__removeat)</ept>.</source>
        </trans-unit>
