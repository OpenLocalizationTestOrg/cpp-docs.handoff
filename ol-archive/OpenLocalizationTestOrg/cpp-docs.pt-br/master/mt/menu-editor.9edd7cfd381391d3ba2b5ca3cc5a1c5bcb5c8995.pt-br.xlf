        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Select Menus and Menu Commands</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Move and Copy Menus and Menu Commands</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>Create Pop-up Menus</source>
