        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>_heapset</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Checks heaps for minimal consistency and sets the free entries to a specified value.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>This function is obsolete.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Beginning in Visual Studio 2015, it is not available in the CRT.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
