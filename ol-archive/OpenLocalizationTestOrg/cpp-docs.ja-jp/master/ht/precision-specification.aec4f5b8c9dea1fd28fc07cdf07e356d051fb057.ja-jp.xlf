        <trans-unit id="137" translate="yes" xml:space="preserve">
          <source>The precision specifies the maximum number of significant digits printed.</source>
        </trans-unit>
        <trans-unit id="138" translate="yes" xml:space="preserve">
          <source>Six significant digits are printed, and any trailing zeros are truncated.</source>
        </trans-unit>
