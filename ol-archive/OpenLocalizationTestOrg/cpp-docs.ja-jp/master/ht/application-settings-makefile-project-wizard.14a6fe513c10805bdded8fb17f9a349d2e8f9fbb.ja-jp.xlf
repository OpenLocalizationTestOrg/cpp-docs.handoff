        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Provide the information for the project.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>The options you specify are reflected in the project's <bpt id="p1">[</bpt>property page<ept id="p1">](../ide/working-with-project-properties.md)</ept>.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Once you create the project, you can view and edit each of the following options in the <bpt id="p1">**</bpt>Nmake<ept id="p1">**</ept> page of the project's property page.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Build command line</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Specifies the command line to run when the user selects <bpt id="p1">**</bpt>Build<ept id="p1">**</ept> from the <bpt id="p2">**</bpt>Build<ept id="p2">**</ept> menu.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Displayed in the <bpt id="p1">**</bpt>Build command line<ept id="p1">**</ept> field on the <bpt id="p2">**</bpt>Nmake<ept id="p2">**</ept> page of the project's property page.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Output</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Specifies the name of the file that will contain the output for the command line.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>By default, this option is based on the project name.</source>
        </trans-unit>
