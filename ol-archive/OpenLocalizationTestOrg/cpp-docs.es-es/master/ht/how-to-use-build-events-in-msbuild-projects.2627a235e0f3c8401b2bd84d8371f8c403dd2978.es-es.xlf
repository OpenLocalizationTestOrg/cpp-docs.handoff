        <trans-unit id="130" translate="yes" xml:space="preserve">
          <source>MSBuild (Visual C++)</source>
        </trans-unit>
        <trans-unit id="131" translate="yes" xml:space="preserve">
          <source>Walkthrough: Using MSBuild to Create a Visual C++ Project</source>
        </trans-unit>
      </group>
